<configuration>
  <appSettings>
    <add key="ValidationSettings:UnobtrusiveValidationMode" value="None" />
  </appSettings>
    <connectionStrings>
        <add name="routeDBConnectionString" connectionString="Data Source=(LocalDB)\v11.0;AttachDbFilename=|DataDirectory|\BTSDB.mdf;Integrated Security=True"
            providerName="System.Data.SqlClient" />
      <add name="BTSDBConn" connectionString="Data Source=(LocalDB)\v11.0;AttachDbFilename=|DataDirectory|\BTSDB.mdf;Integrated Security=True"
        providerName="System.Data.SqlClient" />
    </connectionStrings>
    <system.web>
      <compilation debug="true" targetFramework="4.5" />
      <httpRuntime targetFramework="4.5" />
    </system.web>

    <system.serviceModel>
        <client>
            <endpoint address="http://localhost:6911/PaymentWeb.asmx" binding="basicHttpBinding"
                bindingConfiguration="PaymentWebSoap" contract="PaymentReference.PaymentWebSoap"
                name="PaymentWebSoap" />
        </client>
        <behaviors>
            <serviceBehaviors>
                <behavior name="">
                    <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true" />
                    <serviceDebug includeExceptionDetailInFaults="false" />
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <bindings>
            <basicHttpBinding>
                <binding name="PaymentWebSoap" />
            </basicHttpBinding>
            <customBinding>
                <binding name="PlsCan_Run.Web.Service1.customBinding0">
                    <binaryMessageEncoding />
                    <httpTransport />
                </binding>
            </customBinding>
        </bindings>
        <serviceHostingEnvironment aspNetCompatibilityEnabled="true"
            multipleSiteBindingsEnabled="true" />
        <services>
            <service name="PlsCan_Run.Web.Service1">
                <endpoint address="" binding="customBinding" bindingConfiguration="PlsCan_Run.Web.Service1.customBinding0"
                    contract="PlsCan_Run.Web.Service1" />
                <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
            </service>
        </services>
    </system.serviceModel>
</configuration>
